START:
MOV #1, R0
MOV #2, R1
MOV #3, R2
MOV #4, R3
MOV #5, R4
MOV #3070, SP

JSR R5, SAVE
HALT
SAVE:
MOV R4, -(SP)
MOV R3, -(SP)
MOV R2, -(SP)
MOV R1, -(SP)
MOV R0, -(SP)
MOV #100, R0
MOV #101, R1
MOV #102, R2
MOV #103, R3
MOV #104, R4
BR REST

REST:
TST (SP)+
MOV (SP)+, R0
MOV (SP)+, R1
MOV (SP)+, R2
MOV (SP)+, R3
MOV (SP)+, R4
RTS R5
.END START
