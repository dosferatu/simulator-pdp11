MOV R4, #5

JSR R5, SAVE
SAVE:
MOV R4, -(SP)
MOV R3, -(SP)
MOV R2, -(SP)
MOV R1, -(SP)
MOV R0, -(SP)
MOV R0, #100
MOV R1, #101
MOV R2, #102
MOV R3, #103
MOV R4, #104
BR REST

REST:
TST (SP)+
MOV (SP)+, R0
MOV (SP)+, R1
MOV (SP)+, R2
MOV (SP)+, R3
MOV (SP)+, R4
RTS R5